DELIMITER //

DROP PROCEDURE IF EXISTS INSERT_SALON;

//

CREATE PROCEDURE INSERT_SALON(IN nombreSalon VARCHAR(45))

BEGIN

INSERT INTO Salon (nombre_Salon) VALUES (nombreSalon);

END

